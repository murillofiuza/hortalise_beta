                            android:layout_height="wrap_content"
                            android:id="@+id/nav_alho"
                            android:onClick="garlic"
                            android:background="#f6fef6">

                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="60dp"
                                android:id="@+id/imageViewAlho"
                                android:layout_gravity="left|top"
                                android:src="@drawable/alho" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:text="Garlic"
                                android:id="@+id/textViewAlho"
                                android:layout_gravity="center"
                                android:textColor="#000000"/>

                            <ImageView
                                android:layout_width="37dp"
                                android:layout_height="match_parent"
                                android:id="@+id/imageView14"
                                android:layout_gravity="right|center_vertical"
                                android:src="@drawable/ic_arrow_right"/>
                        </FrameLayout>

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/nav_batata"
                            android:background="#bfffe0"
                            android:onClick="batata">

                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="60dp"
                                android:id="@+id/imageViewBatata"
                                android:layout_gravity="left|top"
                                android:src="@drawable/batata" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:text="Potato"
                                android:id="@+id/textViewBatata"
                                android:layout_gravity="center"
                                android:textColor="#000000"/>

                            <ImageView
                                android:layout_width="37dp"
                                android:layout_height="match_parent"
                                android:id="@+id/imageView15"
                                android:layout_gravity="right|center_vertical"
                                android:src="@drawable/ic_arrow_right"/>
                        </FrameLayout>

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/nav_berinjela"
                            android:onClick="berinjela"
                            android:background="#f6fef6">

                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="60dp"
                                android:id="@+id/imageViewBerinjela"
                                android:layout_gravity="left|top"
                                android:src="@drawable/berinjela" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:text="Eggplant"
                                android:id="@+id/textViewBerinjela"
                                android:layout_gravity="center"
                                android:textColor="#000000"/>

                            <ImageView
                                android:layout_width="37dp"
                                android:layout_height="match_parent"
                                android:id="@+id/imageView16"
                                android:layout_gravity="right|center_vertical"
                                android:src="@drawable/ic_arrow_right"/>
                        </FrameLayout>

                        <FrameLayout
