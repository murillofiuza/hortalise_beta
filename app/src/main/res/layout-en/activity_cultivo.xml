                                android:src="@drawable/cebolinha" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:text="Chive"
                                android:id="@+id/textViewCebolinha"
                                android:layout_gravity="center"
                                android:textColor="#000000"/>

                            <ImageView
                                android:layout_width="37dp"
                                android:layout_height="match_parent"
                                android:id="@+id/imageView27"
                                android:layout_gravity="right|center_vertical"
                                android:src="@drawable/ic_arrow_right"/>
                        </FrameLayout>

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/nav_cenoura"
                            android:onClick="cenoura"
                            android:background="#f6fef6">

                            <ImageView
                                android:layout_width="50dp"
                                android:layout_height="60dp"
                                android:id="@+id/imageViewCenoura"
                                android:layout_gravity="left|top"
                                android:src="@drawable/cenoura" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:text="Carrot"
                                android:id="@+id/textViewCenoura"
                                android:layout_gravity="center"
                                android:textColor="#000000"/>

                            <ImageView
                                android:layout_width="37dp"
                                android:layout_height="match_parent"
                                android:id="@+id/imageViewA11"
                                android:layout_gravity="right|center_vertical"
                                android:src="@drawable/ic_arrow_right"/>
                        </FrameLayout>


                    </LinearLayout>



            </RelativeLayout>




    </android.support.v4.widget.NestedScrollView>
    </android.support.design.widget.CoordinatorLayout>
